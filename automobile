#include<stdio.h>
void main()
{
   int v,w,tw,fw;
   printf("enter the no of vehicles\n");
   scanf("%d",&v);
   printf("enter the no of wheels\n");
   scanf("%d",&w);
   if(w<2&&w%2!=0&&v<w)
   printf("invalid input");
   else
   {
       fw=(w-2*v)/2;
       tw=v-fw;
       printf("two wheeler=%d four wheeler=%d",tw,fw);
   }
   
}
